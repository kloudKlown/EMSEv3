  \edef\@tempa{#2}%
  \def\@tempb{off}%
  \ifx\@tempa\@tempb
    \@chapterlistsgap@off{#1}%
  \else
    \def\@tempb{on}%
    \ifx\@tempa\@tempb
      \@chapterlistsgap@on{#1}%
    \else
      \newfloat@Error{Invalid value `#2' for option `chapterlistsgaps'}%
    \fi
  \fi}
%    \end{macrocode}
%    \begin{macrocode}
\define@key{@newfloat}{chapterlistsgaps}{%
  \def\newfloat@chapterlistsgaps@value{#1}}
